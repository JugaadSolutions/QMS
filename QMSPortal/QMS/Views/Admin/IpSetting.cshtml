@model List<QMS.Models.IP>

@{
    ViewBag.Title = "IpSetting";
}
<h2>IP Setting</h2>
@using (Html.BeginForm("IpSetting", "Admin"))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary()

    <fieldset style="height: 100%; text-align: center;">
        <legend>Login Form</legend>
        @foreach (var item in Model)
        {
            <ol>

                <li style="display: inline-block;">
                    @Html.LabelFor(modelItem=>item.Name,item.Name)
                    @Html.TextBoxFor(modelItem=>item.IP_Address,item.IP_Address)
                </li>
            </ol>
        }
    </fieldset>
    <div>
        <form method="post">
            <input type="submit" value="SAVE" @*onclick="@("window.location.href='" + @Url.Action("IpSetting", "Admin", new {model=Model }) + "'")    ;"*@  />
       </form>
    </div>
}

